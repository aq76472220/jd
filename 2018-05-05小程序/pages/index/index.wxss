/* pages/index/index.wxss */
@import "../../tpl/resourceList/resourceList.wxss"

.ss-banner,.ss-banner-item,.ss-banner-item image{width: 750rpx; height: 250rpx}
.ss-index-nav-item image{display: block; width: 50px; height: 50px; margin: 0 auto}
.ss-index-nav{padding: 15px 0; background: #fff;}
.ss-index-nav-item text{  padding-top: 5px; display: block}

/* 公告 */
.swiper-adv {padding: 15px 10px; background: #fff; height: 20px; padding-left: 35px; padding-right: 60px;} 
.swiper-adv-item {height: 25px;}
.swiper-adv-licon image{display: block; width: 16px; height: 16px;}
.swiper-adv-font{background: url('data:image/png;base64,') no-repeat right center; background-size: 12px; padding-right: 15px;}
.ss-index-tit{ padding: 12px 0; background:#fff url('data:image/png;base64,') no-repeat center center; background-size: 130px 15px;}

.ss-qdsw-box{padding: 20px 0; background: #fff;}
.ss-qdsw-box image {display: block; width: 100px; height: 100px; margin: 0 auto;}
.ss-qdsw-box text{font-size: 14px; color: #c31f23; margin-top: 10px; text-decoration: underline}
.ss-index-bot{background: #fff;padding: 20px 10px;}
.ss-index-bot image{display: block; width: 80px; height: 30px;}