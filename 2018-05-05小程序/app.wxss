/* 登录注册 */
.see-btn{height: 40px; width: 40px; right: 0; background: url('data:image/png;base64,') no-repeat center center; background-size: 20px;}
.see-btn.active{height: 40px; width: 40px; right: 0; background: url('data:image/png;base64,') no-repeat center center; background-size: 20px;}
.ss-sigin-bottom{position: fixed; bottom: 30px;left: 0; right: 0; background: url('data:image/png;base64,') repeat-x left center;}
.ss-sigin-bottom text{padding: 0 10px; background: #fbfbfb}
/* 登录注册 */

/* 申请借款/申请合作 */
.ss-apply-tit{padding: 10px 0;}
.ss-apply-tit text{padding-left: 8px; border-left: 2px solid #c31f23}
.ss-apply-font-con text{line-height: 1.5; padding: 3px 10px;}
.ss-apply-bottom-item .img{display: block; margin: 0 auto; height: 40px; text-align: center}
.ss-apply-bottom-item image{display: block; width: 40px; height: 40px;margin: 0 auto;}
.ss-apply-bottom-item:first-child .img{background-position: 50% center}
.ss-applay-line{left: 0; right: 0; height: 100%; top:0; z-index: -1;}
.ss-applay-line-item{height: 40px;background: url('data:image/png;base64,') repeat-x left center; background-size: 4px 1px;width: 50%}
.ss-apply-bottom-item:first-child .ss-applay-line .ss-applay-line-item:first-child{background: none}
.ss-apply-bottom-item:last-child .ss-applay-line .ss-applay-line-item:last-child{background: none}
/* 申请借款/申请合作 */

