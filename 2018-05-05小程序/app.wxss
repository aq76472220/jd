/*主要线 start*/
.t-line:before{position: absolute;top:0; left: 0px; content:"";border-top: 1px solid #D9D9D9; -webkit-transform-origin:0 0;transform-origin:0 0;transform:scaleY(1); width: 100%; height: 1px;-webkit-transform:scaleY(1); z-index: 2}/* 头部线 */
